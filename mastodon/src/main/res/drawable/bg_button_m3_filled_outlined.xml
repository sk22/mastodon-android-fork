<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="@color/m3_pressed_overlay" xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:gravity="center_vertical" android:height="40dp">
		<selector>
			<item android:state_enabled="true">
				<shape>
					<solid android:color="?colorM3Primary"/>
					<corners android:radius="20dp"/>
				</shape>
			</item>
			<item>
				<shape>
					<stroke android:color="@color/m3_on_surface_overlay" android:width="1dp"/>
					<corners android:radius="20dp"/>
				</shape>
			</item>
		</selector>
	</item>
</ripple>